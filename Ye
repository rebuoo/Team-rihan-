<script>

if (document.getElementsByClassName('status_checker')) {

    
	
	
	function checkStatus() {
    const input = document.getElementById('u-statusInput').value.toLowerCase();
    const statusCard = document.getElementById('u-statusCard');
    const getInfoBtn = document.getElementById('getInfoBtn');

    if (!input) {
        statusCard.textContent = "Please enter a valid input.";
        statusCard.className = 'u-card failure';
        statusCard.style.display = 'block';
        return;
    }

    // Disable button and show loading state
    getInfoBtn.disabled = true;
    getInfoBtn.textContent = "Loading...";

    const apiUrl = "https://smmbin.com/api";
    const apiKey = "your-api-key-here"; // Make sure to provide a valid API key
    const post_data = 'apiKey=' + apiKey + '&actionType=verify-spam-checker-free&link=' + input;
    
    const xhttp = new XMLHttpRequest();
    xhttp.onreadystatechange = function () {
        if (this.readyState === 4) {
            // Re-enable the button after request is complete
            getInfoBtn.disabled = false;
            getInfoBtn.textContent = "Get Info";

            if (this.status === 200) {
                try {
                    const data = JSON.parse(this.responseText);

                    console.log(data);
					
					 console.log(data.data);
					 
					console.log(data.data.data);

                    if (data.success) {
                        const spamSetting = data.data.data.spam_setting;
                        console.log("Spam Setting:", spamSetting);  // Log the value for debugging
                        
                        // Ensure we're comparing correctly (check for number or string type)
                        if (spamSetting === 1 || spamSetting === '1') {
                            statusCard.textContent = "Spam filter is on.";
                            statusCard.className = 'u-card failure';
                        } else if (spamSetting === 0 || spamSetting === '0') {
                            statusCard.textContent = "Spam filter is off.";
                            statusCard.className = 'u-card';
                        } else {
                            statusCard.textContent = "Unexpected spam setting value: " +  this.responseText;
                            statusCard.className = 'u-card failure';
                        }
                    } else {
                        statusCard.textContent = 'Error: ' +  this.responseText;
                        statusCard.className = 'u-card failure';
                    }
                } catch (error) {
                    statusCard.textContent = 'Error parsing data: ' +  this.responseText;
                    statusCard.className = 'u-card failure';
                }
            } else {
                statusCard.textContent = 'Request failed. Status code: ' + this.responseText;
                statusCard.className = 'u-card failure';
            }

            statusCard.style.display = 'block';
        }
    };

    xhttp.open("POST", apiUrl, true);
    xhttp.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
    xhttp.send(post_data);
}



    // Function to fetch followers count
    function fetchFollowers() {
        const input = document.getElementById('u-userInput').value.toLowerCase();
        const userCard = document.getElementById('u-userCard');
        const fetchButton = document.querySelector('#u-rightBox button');

        if (!input) {
            userCard.textContent = "Please enter a valid username or link.";
            userCard.className = 'u-card failure';
            userCard.style.display = 'block';
            return;
        }

        // Disable button and show loading state
        fetchButton.disabled = true;
        fetchButton.textContent = "Loading...";

        const xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function () {
            if (this.readyState === 4) {
                fetchButton.disabled = false;
                fetchButton.textContent = "Get Info";

                if (this.status === 200) {
                    try {
                        const data = JSON.parse(this.responseText);
						
                        if (data.data.data.followers) {
                            
                            userCard.innerHTML = `
                                <p><strong>Followers:</strong> ${data.data.data.followers}</p>
                            `;
                            userCard.className = 'u-card';
                        } else {
                            
							if (data.data.data.spam_setting == 1  || data.data.data.spam_setting == '1' ) {
                                statusCard.textContent = "Spam filter is on.";
								statusCard.className = 'u-card failure';
								
                            } else if( data.data.data.spam_setting == 0  || data.data.data.spam_setting == '0' ) {
                                
								statusCard.textContent = "Spam filter is off.";
								statusCard.className = 'u-card';
								
                            }else{
							
								statusCard.textContent = "Error parsing data";
								statusCard.className = 'u-card failure';
								
							}
							
                        }
                    } catch (error) {
                        statusCard.textContent = 'Error parsing data '+this.responseText;
                    statusCard.className = 'u-card failure';
                    }
                } else {
                    statusCard.textContent = 'Error parsing data '+this.responseText;
                    statusCard.className = 'u-card failure';
                }

                userCard.style.display = 'block';
            }
        };

        const post_data = "apiKey=&actionType=verify-spam-checker-free&link="+input;

        xhttp.open("POST", "https://smmbin.com/api", true);
        xhttp.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
        xhttp.send(post_data);
    }

}
</script>
